info:
    title: Application
    author: Google, LLC

required:
    - billing_account_id
    - root_node
    - dev_node
    - prd_node
    
properties:
    enabled_services:
        type: array
        uniqueItems: true
        items:
            type: string
    
    billing_account_id:
        type: string

    root_node:
        type: integer

    dev_node:
        type: integer

    prd_node:
        type: integer

    use_network_project:
        type: boolean
        default: true