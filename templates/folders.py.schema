info:
    title: Application
    author: Google, LLC

required:
    - parent_node
    
properties:
    parent_node:
        type: object
        required:
            - type
            - id
        properties:
            type:
                type: string
                enum:
                    - organization
                    - folder
            id:
                type: [integer, string]
                pattern: ^[0-9]{8,25}$